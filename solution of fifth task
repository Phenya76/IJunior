class Program
    {
        public static void Main(string[] args)
        {
             string firstName = "Парфентьев";
            string lastName = "Александр";
            Console.WriteLine(firstName + " " + lastName);

            string exchange = firstName;
            firstName = lastName;
            lastName = exchange;

            Console.WriteLine(firstName + " " + lastName);
            Console.ReadLine();
        }
    }
